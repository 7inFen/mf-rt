{
  "name": "mf-react-template",
  "version": "1.0.0",
  "description": "MediaFull React Template",
  "engines": {
    "node": ">=8",
    "npm": ">=3"
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "scripts": {
    "start-message": "babel-node tools/startMessage.js",
    "remove:cache": "rimraf ./node_modules/.cache",
    "start": "concurrently -k -r -s first \"npm run mocks\" \"npm run open:src\"",
    "start:dev": "concurrently -k -r -s first \"npm run open:preSrc\"",
    "restart": "npm run remove:cache && npm run start",
    "restart:dev": "npm run remove:cache && npm run start:dev",
    "open:src": "babel-node --max-old-space-size=4096 tools/srcServer.js",
    "open:preSrc": "babel-node --max-old-space-size=4096 tools/preServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "remove-dist": "rimraf ./dist",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "build:static": "npm run clean-dist && babel-node tools/build.js --development && npm run open:dist",
    "build:dev": "npm run clean-dist && babel-node tools/build.js --predev",
    "build": "npm run clean-dist && babel-node tools/build.js --production",
    "test": "jest",
    "test:CI": "jest",
    "test:cover": "npm run test -- --coverage ",
    "test:cover:CI": "npm run test:CI -- --coverage && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "jest --watchAll",
    "open:cover": "npm run test:cover && open-cli ./coverage/lcov-report/index.html",
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js",
    "mocks": "babel-node ./mocks/json-server.js"
  },
  "author": "MF FE",
  "license": "MIT",
  "dependencies": {
    "@ant-design/icons": "4.6.2",
    "@antv/data-set": "0.11.8",
    "@babel/runtime": "7.14.8",
    "@hot-loader/react-dom": "17.0.1",
    "@loadable/component": "5.15.0",
    "ahooks": "2.10.9",
    "antd": "4.16.8",
    "axios": "0.21.1",
    "bizcharts": "4.1.11",
    "classnames": "2.3.1",
    "connected-react-router": "6.9.1",
    "history": "4.10.1",
    "immutable": "4.0.0-rc.14",
    "js-file-download": "0.4.12",
    "moment": "2.29.1",
    "normalize.css": "8.0.1",
    "object-assign": "4.1.1",
    "qrcode.react": "1.0.1",
    "qs": "6.10.1",
    "query-string": "5.1.1",
    "react": "17.0.2",
    "react-copy-to-clipboard": "5.0.3",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "react-puzzle-confirm": "1.0.17",
    "react-redux": "7.2.4",
    "react-router-dom": "5.2.0",
    "react-visibility-sensor": "5.1.1",
    "react-window": "1.8.6",
    "react-window-infinite-loader": "1.0.7",
    "rebass": "4.0.7",
    "redux": "4.1.0",
    "redux-auth-wrapper": "3.0.0",
    "redux-thunk": "2.3.0",
    "store": "2.0.12"
  },
  "devDependencies": {
    "@babel/cli": "7.14.8",
    "@babel/core": "7.14.8",
    "@babel/eslint-parser": "7.14.7",
    "@babel/node": "7.14.7",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.14.5",
    "@babel/plugin-proposal-optional-chaining": "7.14.5",
    "@babel/plugin-proposal-pipeline-operator": "7.14.8",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-react-constant-elements": "7.14.5",
    "@babel/plugin-transform-runtime": "7.14.5",
    "@babel/polyfill": "7.8.7",
    "@babel/preset-env": "7.14.8",
    "@babel/preset-react": "7.14.5",
    "@pmmmwh/react-refresh-webpack-plugin": "0.4.3",
    "@quickbaseoss/babel-plugin-styled-components-css-namespace": "1.0.1",
    "@types/react": "17.0.15",
    "@types/react-dom": "17.0.9",
    "@typescript-eslint/eslint-plugin": "4.28.5",
    "@typescript-eslint/parser": "4.28.5",
    "acorn": "8.4.1",
    "ajv": "6.12.2",
    "autoprefixer": "10.3.1",
    "babel-core": "7.0.0-bridge.0",
